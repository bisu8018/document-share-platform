{
  "name": "decompanyio_front_end",
  "description": "Polarishare.com",
  "version": "0.1.0",
  "license": "ISC",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@sentry/browser": "^5.0.6",
    "auth0-js": "^9.10.1",
    "axios": "^0.18.0",
    "bignumber.js": "^8.1.1",
    "classnames": "latest",
    "drizzle": "^1.3.3",
    "history": "^4.9.0",
    "jquery": "^3.3.1",
    "js-file-download": "^0.4.5",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.6",
    "react-drag-drawer": "^3.3.0",
    "react-full-screen": "^0.2.4",
    "react-ga": "^2.5.7",
    "react-google-charts": "^3.0.11",
    "react-helmet": "^5.2.0",
    "react-infinite-scroll-component": "^4.5.2",
    "react-lines-ellipsis": "^0.14.1",
    "react-linkify": "^0.2.2",
    "react-redux": "^6.0.1",
    "react-responsive-carousel": "^3.1.47",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "react-share": "^2.4.0",
    "react-spinkit": "^3.0.0",
    "react-spinners": "^0.5.3",
    "react-tabs": "^3.0.0",
    "react-tagsinput": "^3.19.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.14",
    "web3": "^1.0.0-beta.52"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "set PORT=8000 && react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "aws s3 sync build/ s3://share.decompany.io && npm run invaildate:dev",
    "deploy:production": "aws s3 sync build/ s3://www.polarishare.com && npm run invaildate:production",
    "invaildate:dev": "aws cloudfront create-invalidation --distribution-id EDFUPNJU9XKGX --paths /*",
    "invaildate:production": "aws cloudfront create-invalidation --distribution-id E1UYELY2K59G6Q --paths /*",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
