{
  "name": "decompanyio_front_end",
  "description": "Decompany.io Project",
  "version": "0.1.0",
  "license": "ISC",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "auth0-js": "^9.10.0",
    "axios": "^0.18.0",
    "bignumber.js": "^8.0.2",
    "classnames": "latest",
    "drizzle": "^1.3.3",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "js-file-download": "^0.4.4",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "prop-types": "^15.7.2",
    "qs": "^6.6.0",
    "react": "^16.8.1",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.1",
    "react-drag-drawer": "^3.3.0",
    "react-full-screen": "^0.2.3",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.0",
    "react-infinite-scroll-component": "^4.5.0",
    "react-linkify": "^0.2.2",
    "react-meta-tags": "^0.7.4",
    "react-redux": "^6.0.1",
    "react-responsive-carousel": "^3.1.47",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "react-spinkit": "^3.0.0",
    "react-spinners": "^0.5.1",
    "react-tabs": "^3.0.0",
    "react-tagsinput": "^3.19.0",
    "redux-actions": "^2.6.4",
    "shortid": "^2.2.14",
    "web3": "^1.0.0-beta.46"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "set PORT=8000 && react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "aws s3 sync build/ s3://dev.decompany.io",
    "deploy:production": "aws s3 sync build/ s3://share.decompany.io && npm run invaildate",
    "invaildate": "aws cloudfront create-invalidation --distribution-id EDFUPNJU9XKGX --paths '/*'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
